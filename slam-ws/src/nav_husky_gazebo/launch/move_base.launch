<launch>
  <arg name="global_costmap_params" default="$(find nav_husky_gazebo)/config/global_costmap_params.yaml"/> 
  <arg name="caffeine" default='false'/> 

  <master auto="start"/>

  <!-- Run Move Base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find nav_husky_gazebo)/config/caffeine_costmap_common_params.yaml" command="load" ns="global_costmap" if="$(arg caffeine)" /> 
    <rosparam file="$(find nav_husky_gazebo)/config/caffeine_costmap_common_params.yaml" command="load" ns="local_costmap" if="$(arg caffeine)" />
    
    <rosparam file="$(find nav_husky_gazebo)/config/husky_costmap_common_params.yaml" command="load" ns="global_costmap" unless="$(arg caffeine)" /> 
    <rosparam file="$(find nav_husky_gazebo)/config/husky_costmap_common_params.yaml" command="load" ns="local_costmap" unless="$(arg caffeine)" />
    
    <rosparam file="$(find nav_husky_gazebo)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(arg global_costmap_params)" command="load" /> 
    <rosparam file="$(find nav_husky_gazebo)/config/base_local_planner_params.yaml" command="load" />
    
    <!-- Set control loop frequency to an appropriate rate -->
    <param name="controller_frequency" value="10.0" />
  </node> 
</launch> 
